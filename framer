# Oefening favoriet1 bekijken "Ademhaling tellen"

sketch.klikf1.onClick (event, layer) ->
	sketch.scherm3.animate "active",
		curve: "spring(300, 20, 0)"
		sketch.favorietenscherm.stateSwitch "uit"
sketch.scherm3.states.active =
	x:0
	y:0

