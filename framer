sketch.favorieten3.onClick (event, layer) ->
	sketch.meldingenscherm.stateSwitch "uit"
	sketch.testscherm.stateSwitch "default"

# Naar Oefeningen
sketch.oefeningen3.onClick (event, layer) ->
	sketch.meldingenscherm.stateSwitch "uit"
	sketch.scherm1.stateSwitch "default"


