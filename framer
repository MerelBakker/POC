sketch.fijnklik.states.aan =
	x: 30
	y: 300
	
sketch.fijn.states.uit =
	x: 1000
	y: 1000
	
sketch.fijn.onClick (event, layer) ->
	sketch.fijnklik.stateSwitch "aan"
	sketch.fijn.stateSwitch "uit"


